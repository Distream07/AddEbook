{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "styles.css",
        "use": "@vercel/static"
      },
      {
        "src": "script.js",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/add-images",
        "dest": "/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  